async function checkLocalTables() {
  console.log('ðŸ” Checking what tables exist in local database...\n');
  
  const existingLocal = [];
  const missingLocal = [];
  
  console.log(`ðŸ“Š Testing ${ALL_REMOTE_TABLES.length} tables...`);
  
  for (const tableName of ALL_REMOTE_TABLES) {
    try {
      const { data, error } = await localSupabase
        .from(tableName)
        .select('*')
        .limit(1);
      
      if (!error) {
        existingLocal.push(tableName);
        process.stdout.write('âœ…');
      } else {
        missingLocal.push(tableName);
        process.stdout.write('âŒ');
      }
    } catch (e) {
      missingLocal.push(tableName);
      process.stdout.write('âŒ');
    }
  }
  
  console.log('\n\n' + '='.repeat(80));
  console.log('ðŸ“Š LOCAL DATABASE STATUS');
  console.log('='.repeat(80));
  console.log(`âœ… Tables that exist locally: ${existingLocal.length}`);
  console.log(`âŒ Tables missing locally: ${missingLocal.length}`);
  console.log(`ðŸ“Š Total remote tables: ${ALL_REMOTE_TABLES.length}`);
  
  if (existingLocal.length > 0) {
    console.log('\nâœ… EXISTING LOCAL TABLES:');
    existingLocal.forEach((table, i) => {
      console.log(`${i + 1}. ${table}`);
    });
  }
  
  if (missingLocal.length > 0) {
    console.log('\nâŒ MISSING LOCAL TABLES:');
    missingLocal.forEach((table, i) => {
      console.log(`${i + 1}. ${table}`);
    });
  }
  
  console.log('\nðŸŽ¯ CONCLUSION:');
  if (missingLocal.length === ALL_REMOTE_TABLES.length) {
    console.log('ðŸ’¥ NO REMOTE TABLES exist locally! Your databases are completely out of sync.');
    console.log('ðŸš¨ CRITICAL: You should NOT delete the remote database yet!');
  } else if (missingLocal.length > 0) {
    console.log(`âš ï¸  ${missingLocal.length} tables are missing locally.`);
    console.log('ðŸš¨ You should copy these table structures before deleting remote database.');
  } else {
    console.log('âœ… All remote tables exist locally. Safe to proceed with deletion after data sync.');
  }
  
  return { existingLocal, missingLocal };
}

